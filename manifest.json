{
	"manifest_version": 3,
	"name": "Search Google Images from Text",
	"version": "1.0",
	"description": "Right-click selected text to search Google Images instantly.",
	"permissions": [
		"contextMenus"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	},
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"author": "FGomezCode",
	"homepage_url": "https://github.com/Fgomezcode"
}